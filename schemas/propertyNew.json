{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.property.com/propertyNew.schema.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1
    },
    "host_username": {
      "type": "string",
      "minLength": 1,
      "maxLength": 25
    },
    "image": {
      "type": "string"
    },
    "price_night": {
      "type": "integer",
      "minimum": 1
    },
    "description": {
      "type": "string"
    },
    "address": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "title",
    "host_username",
    "image",
    "price_night",
    "description",
    "address"
  ]
}
