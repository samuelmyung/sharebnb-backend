{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.property.com/propertyNew.schema.json",
  "type": "object",
  "properties": {
    "guest_username": {
      "type": "string",
      "minLength": 1,
      "maxLength": 25
    },
    "property_id": {
      "type": "integer"
    },
    "checkin_date": {
      "type": "string",
      "format": "date-time"
    },
    "checkout_date": {
      "type": "string",
      "format": "date-time"
    },
    "date_booked": {
      "type": "string",
      "format": "date-time"
    },
    "price_night": {
      "type": "integer",
      "minimum": 1
    }
  },
  "additionalProperties": false,
  "required": [
    "guest_username",
    "property_id",
    "checkin_date",
    "checkout_date",
    "date_booked",
    "price_night"
  ]
}
